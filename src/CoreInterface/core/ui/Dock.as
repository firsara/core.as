package core.ui{	import core.api.Lib;	import flash.display.DisplayObject;	import flash.display.Sprite;	import flash.display.Bitmap;		public class Dock extends Sprite {				private var _background:DisplayObject;				public function Dock()		{			_background = Lib.get('ui/dock/dock.background');			Bitmap(_background).smoothing = true;			addChild(_background);		}			}}