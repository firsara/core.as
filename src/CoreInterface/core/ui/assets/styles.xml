<?xml version="1.0" encoding="UTF-8"?><Styles>		<!--		UI        ->    Helvetica Neue		UIBold    ->    Helvetica Neue bold	-->		<Fonts>		<Font>			<name><![CDATA[UI]]></name>			<font><![CDATA[UIFont]]></font>			<bold>false</bold>		</Font>		<Font>			<name><![CDATA[UIBold]]></name>			<font><![CDATA[UIFont]]></font>			<bold>true</bold>		</Font>	</Fonts>			<Default>		<font>UI</font>		<size>14</size>		<color>0x080808</color>		<align>left</align>		<TextField>			<autoSize>left</autoSize>			<sharpness>50</sharpness>			<thickness>-20</thickness>		</TextField>	</Default>		<DefaultUI>		<font>UI</font>		<size>14</size>		<color>0x080808</color>		<align>left</align>		<TextField>			<autoSize>left</autoSize>			<sharpness>50</sharpness>			<thickness>-20</thickness>		</TextField>	</DefaultUI>			<Style>		<name><![CDATA[Console]]></name>		<font>UI</font>		<size>14</size>		<color>0xEEEEEE</color>		<align>left</align>	</Style>		<!-- CHECKBOX -->	<Style>		<name><![CDATA[CheckBox]]></name>		<font>UI</font>		<size>16</size>		<color>0x101010</color>	</Style>		<Style>		<name><![CDATA[CheckBox.off]]></name>		<font>UI</font>		<size>16</size>		<color>0x555555</color>	</Style><!-- CHECKBOX -->	<!-- ALERT -->	<Style>		<name><![CDATA[Alert.button]]></name>		<font>UI</font>		<size>20</size>		<color>0xEEEEEE</color>	</Style>	<Style>		<name><![CDATA[Alert.title]]></name>		<font>UIBold</font>		<size>20</size>		<color>0xFFFFFF</color>	</Style>	<Style>		<name><![CDATA[Alert.text]]></name>		<font>UI</font>		<size>16</size>		<color>0xFFFFFF</color>	</Style><!-- ALERT --><!-- RADIOBUTTON -->	<Style>		<name><![CDATA[RadioButton]]></name>		<font>UI</font>		<size>16</size>		<color>0x101010</color>	</Style>		<Style>		<name><![CDATA[RadioButton.off]]></name>		<font>UI</font>		<size>16</size>		<color>0x555555</color>	</Style><!-- RADIOBUTTON --><!-- PICKER -->	<Style>		<name><![CDATA[Roller, Picker]]></name>		<font>UIBold</font>		<size>24</size>		<color>0x050505</color>	</Style><!-- PICKER --><!-- VIDEO PLAYER -->	<Style>		<name><![CDATA[VideoPlayer]]></name>		<font>UIBold</font>		<size>16</size>		<color>0xFFFFFF</color>	</Style><!-- VIDEO PLAYER -->		</Styles>